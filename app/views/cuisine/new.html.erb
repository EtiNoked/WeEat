<%= link_to("<< Go to Cuisines List", cuisine_index_path, :class => 'back-line') %>

<div class="cuisine new">
  <h2>Create Cuisine</h2>

  <%= form_for(@cuisine, :url => cuisine_index_path) do |f| %>

    <%= render(:partial => 'cuisine_form', :locals => {:f => f}) %>

    <div class="form-buttons">
      <%= f.submit("Create Cuisine") %>
    </div>
  <% end %>
</div>